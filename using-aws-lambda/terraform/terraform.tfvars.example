# Example Terraform variables file
# Copy this to terraform.tfvars and customize the values

# AWS Configuration
aws_region = "us-east-1"
environment = "dev"

# Resource Names (customize these to be unique)
dynamodb_table_name = "csv-data-table"
lambda_function_name = "csv-processor-lambda"
s3_bucket_name = "my-unique-csv-bucket-12345"  # Must be globally unique!
iam_role_name = "csv-processor-lambda-role"

# Additional tags
tags = {
  Project = "CSV-to-DynamoDB-ETL"
  Owner   = "YourName"
  Cost    = "Development"
}

